#include <iostream>
using namespace std;

struct Node
{
    int data;
    Node *next;
};

Node *start=NULL;

void printNode(Node *n)
{
    Node *temp;
    temp=n;
    
    cout<<temp->data<<" ";
    while(temp->next!=NULL)
    {
        temp=temp->next;
        cout<<temp->data<<" ";
    }
    cout<<endl;
}

int main()
{
    Node *temp=new Node();
    temp->data= 1;
    temp->next=NULL;
    start=temp;
    
    Node *second= new Node();
    second->data=2;
    second->next=NULL;
    temp->next=second;
    
    Node *third= new Node();
    third->data=3;
    third->next=NULL;
    second->next=third;
    
    printNode(start);
    
    return 0;
}
